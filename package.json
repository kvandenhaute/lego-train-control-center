{
  "name": "lego-train-control-center",
  "description": "Temptation",
  "homepage": "https://github.com/kvandenhaute/lego-train-control-center",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "test": "jest",
    "test:coverage": "npx open ./coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "tsc:compile": "tsc --project .",
    "tsc:validate": "tsc -w --project ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kvandenhaute/lego-train-control-center.git"
  },
  "author": "Kevin Van Den Haute",
  "license": "MIT",
  "engines": {
    "node": "^14.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "./scripts/git/prepare-commit-msg.sh $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --ext .ts --cache --fix"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "brace": "0.11.1",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "sirv": "1.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.10",
    "@babel/runtime": "7.12.5",
    "@rollup/plugin-babel": "5.2.2",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-typescript": "6.1.0",
    "@rollup/plugin-url": "5.0.1",
    "@testing-library/svelte": "3.0.0",
    "@tsconfig/svelte": "1.0.10",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.18",
    "@types/node": "14.14.12",
    "@types/web-bluetooth": "0.0.9",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "autoprefixer": "10.1.0",
    "eslint": "7.15.0",
    "eslint-plugin-svelte3": "2.7.3",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "lint-staged": "10.5.3",
    "node-sass": "5.0.0",
    "npm-check-updates": "10.2.3",
    "open": "7.3.0",
    "postcss": "8.2.1",
    "rollup": "2.34.2",
    "rollup-plugin-svelte": "6.1.1",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.28.10",
    "stylelint": "13.8.0",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "svelte": "3.31.0",
    "svelte-check": "1.1.20",
    "svelte-jester": "1.2.0",
    "svelte-preprocess": "4.6.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "tslib": "2.0.3",
    "typescript": "4.1.2",
    "utility-types": "3.10.0"
  }
}
